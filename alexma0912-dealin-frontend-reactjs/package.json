  "scripts": {
    "test": "npx tsc -w",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.@(j|t)s?(x)\"",
    "lint": "eslint --ext \"src/**/*.@(j|t)s?(x)\"",
    "lint:fix": "eslint --fix \"src/**/*.@(j|t)s?(x)\"",
    "prettier": "prettier --check \"src/**/*.@(j|t)s?(x)\"",
    "prettier:fix": "prettier --write \"src/**/*.@(j|t)s?(x)\"",
    "start": "npm run lint && npx tsc && react-scripts start --config",
    "build": "set CI=false && npx tsc && react-scripts build --config",
    "dev": "concurrently \"npm run lint:fix\" \"npm run prettier:fix\" \"npx tsc -w\" \"react-scripts start --config\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.(ts|tsx)": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "airbnb/hooks"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/google-fonts": "^1.0.0",
    "@types/google-map-react": "^2.1.7",
